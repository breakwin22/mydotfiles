export OSCAR_PROFILE_PATH=~/.oscar_exports
source $OSCAR_PROFILE_PATH
function tffmt() {
  local dir='.'
  local version=''
  while getopts 'p:v:' flag; do
    case "${flag}" in
      p) dir="${OPTARG}" ;;
      v) version="${OPTARG}" ;;
      *) echo "Unexpected option ${flag}" && return;;
    esac
  done
  if [ -z "$version" ]; then
    echo "Attempting to automatically determine tf version"
    local versionfile=""
    if [[ -d "$dir" ]]; then
      versionfile="$dir"/versions.tf
    else
      versionfile="$(dirname $dir)"/versions.tf
    fi
    if [[ ! -e "$versionfile" ]]; then
      echo "Unable to find $versionfile!"
      return
    fi
    version=$(awk '/required_version/ {gsub(/"[v]*/, "", $3); print $3}' "${versionfile}")
    echo "Found version: $version"
  fi
  dir="$(
    cd "$(dirname "$dir")" || return
    pwd
  )"
  echo "Running 'terraform fmt'..."
  eval docker run --rm -it \
    -v "$dir:$dir" \
    -w "$dir" \
    hashicorp/terraform:"$version" fmt
}

